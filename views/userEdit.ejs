<form action="/user/edit/<%=editUser._id%>" method="post">
    <input type="email" name="email" id="email" value="<%=editUser.email%>">
    <input type="text" name="name" id="name" value="<%=editUser.name%>">
    <input type="text" name="company" id="company" value="<%=editUser.company%>">
    <input type="text" name="phone" id="phone" value="<%=editUser.phone%>">
    <input type="number" name="role" id="role" value="<%=editUser.role%>">
    <input type="submit">
</form>
